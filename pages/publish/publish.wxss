.container{
  padding: 40rpx 60rpx;
}

.container .textarea-view{
  width: 100%;
  height: 200rpx;
  font-size: 28rpx;
  border: 1px solid #ccc;
}

.image-list-group{
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  margin-bottom: 40rpx;
  margin-top: 20rpx;
}

.image-list-group .image-list{
  margin-right: 5rpx;
  margin-bottom: 5rpx;
  position: relative;
}

.image-list-group .image-list .item-image{
  width: 100%;
  height: 100%;
 
}

.image-list-group .image-list .remove-image{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  right: -15rpx;
  top: -15rpx;
  z-index: 1;
  background-size: 50rpx;
  background-repeat: no-repeat;
  background-position: center;
  background-image: url("data:image/png;base64,");
}

.image-list-group .add-image-btn view{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 100%;
  font-size: 100rpx;
  background-color: #ccc;
  color: #ffffff;
}